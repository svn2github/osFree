#ifndef scbase_idl
#define scbase_idl

#include <scentry.idl>
interface SOMTClassEntryC;

interface SOMTBaseClassEntryC : SOMTEntryC
{
  readonly attribute SOMTClassEntryC somtBaseClassDef;
  // Returns the class definition entry for the Base class named in
  // this entry.
#ifdef __SOMIDL__
  implementation {
    releaseorder: _get_somtBaseClassDef;
  
    //# Class Modifiers
    majorversion = 2;
    minorversion = 1;
    filestem = scbase;
#ifdef _WIN32
    dllname = "some.dll";
#endif
    callstyle = oidl;
  
    //# Method Modifiers
    //somDumpSelfInt: override;
    //somtSetSymbolsOnEntry: override;
  
    //# Attribute Modifiers
    somtBaseClassDef: nodata;
  };
#endif /* __SOMIDL__ */
};


#endif  /* scbase_idl */
